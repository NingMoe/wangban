<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <defaultCache
        maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="1200"
        timeToLiveSeconds="1200"
        overflowToDisk="true" />
   <cache name="VerifyCode" maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="180"
        timeToLiveSeconds="180"
        overflowToDisk="false"/>
   <cache name="MessageCode" maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="180"
        timeToLiveSeconds="180"
        overflowToDisk="false" />
    <cache name="EmailCode" maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="180"
        timeToLiveSeconds="180"
        overflowToDisk="false" />
   <cache name="ValidMail" maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="900"
        timeToLiveSeconds="1800"
        overflowToDisk="false" />
   <cache name="MailIndex" maxElementsInMemory="1000"
        eternal="false"
        diskPersistent="true"
        timeToIdleSeconds="900"
        timeToLiveSeconds="1800"
        overflowToDisk="false" />    
   <cache name="ICITYSession" maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="3600" 
   		timeToLiveSeconds="7200"
   		overflowToDisk="true" 
   		diskPersistent="true"></cache>
  <cache name="icity_sms" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="300"
   		timeToLiveSeconds="300"
   		overflowToDisk="false" 
   		diskPersistent="false"/>
  <cache name="StaticFile" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="300"
   		timeToLiveSeconds="300"
   		overflowToDisk="true" 
   		diskPersistent="false"/>
    <cache name="icity_businessStat" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="1800"
   		timeToLiveSeconds="1800"
   		overflowToDisk="false" 
   		diskPersistent="false"/>
   	<cache name="icity_businessListTab" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="43200"
   		timeToLiveSeconds="43200"
   		overflowToDisk="false" 
   		diskPersistent="false"/>
   	<cache name="login_fail_time" maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="3600" 
   		timeToLiveSeconds="3600"
   		overflowToDisk="false" 
   		diskPersistent="false"></cache>
    <cache name="access_token" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="3600"
   		timeToLiveSeconds="7200"
   		overflowToDisk="false" 
   		diskPersistent="false"/>
   	<cache name="ProxySignature" 
   		maxElementsInMemory="1000" 
   		eternal="false" 
   		timeToIdleSeconds="3600"
   		timeToLiveSeconds="7200"
   		overflowToDisk="false" 
   		diskPersistent="false"/>
</ehcache>